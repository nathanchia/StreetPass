{
  "expo": {
    "name": "CloCards",
    "description": "Retrieve cards written by other users close to your location",
    "slug": "clo-cards",
    "version": "1.0.0",
    "platforms": ["ios", "android"],
    "orientation": "portrait",
    "icon": "./assets/Logo.png",
    "splash": {
      "backgroundColor": "#ffffff",
      "resizeMode": "cover",
      "image": "./assets/Splash.png"
    },
    "ios": {
      "bundleIdentifier": "com.nathanchia.clocards",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.nathanchia.clocards",
      "versionCode": 1,
      "permissions": ["ACCESS_COARSE_LOCATION", "ACCESS_FINE_LOCATION"]
    }
  }
}
